{
    "cache_profile": {
        "default_cache": {
            "cache_path": "/etc/icaro_cache", 
            "inactive": "60m", 
            "keys_zone": "mykey", 
            "levels": "1:2", 
            "max_size": "10g", 
            "use_temp_path": "off"
        }
    }, 
    "config_profiles": {
        "default": {
            "proxy_set_header": [
                "Host $http_host", 
                "X-Real-IP $remote_addr", 
                "X-Forwarded-For $proxy_add_x_forwarded_for", 
                "X-Forwarded-Proto $scheme"
            ]
        }
    },
    "machines":{
        "secondNode":{
            "addr": "192.168.0.13",
            "port": 22,
            "username": "user1",
            "password": "cool_password"
        }
    },
