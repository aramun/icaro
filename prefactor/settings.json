        {
	    "packages":["icaro"],
            "apis": [
                {
                    "cache_profile": "default_cache", 
                    "config_profile": "default", 
                    "name": "api1", 
                    "roles": [
                        "all"
                    ], 
                    "versions":["1.2"],
                    "current_version": "1.2"
                }, 
                {
                    "cache_profile": "default_cache", 
                    "config_profile": "default", 
                    "name": "api3", 
